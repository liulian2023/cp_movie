<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <com.uyt.ying.yuan.uuuu.iuymn.mkjnb.myView.FloatingDraftButton
        android:id="@+id/floatingActionButton"
        android:layout_height="40dp"
        android:layout_width="40dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        />
    <ImageView
        android:id="@+id/chess_refresh_iv"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:src="@drawable/shuax_btn"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:visibility="invisible"
        />
    <ImageView
        android:id="@+id/chess_return_iv"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:src="@drawable/gc_btn"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:visibility="invisible"
        />
    <ImageView
        android:id="@+id/chess_change_quota_iv"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:src="@drawable/edzh_btn"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:visibility="invisible"
        />

    <ImageView
        android:id="@+id/rotation_iv"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:src="@drawable/spzhp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:visibility="invisible"
        />
</RelativeLayout>
