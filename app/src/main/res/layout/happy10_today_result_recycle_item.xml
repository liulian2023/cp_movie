<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        android:baselineAligned="false" android:gravity="center"
        android:orientation="horizontal"

        >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:orientation="vertical"
            android:layout_gravity="center">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="1923562"
                android:textSize="12sp"
                android:id="@+id/qishu"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="20:40:25"
                android:gravity="center"
                android:layout_marginTop="3dp"
                android:id="@+id/time"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:gravity="center"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center">
                    <com.uyt.ying.yuan.uuuu.iuymn.mkjnb.myView.MyCornerTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:text="10"
                        android:gravity="center"
                        android:id="@+id/happy10_num_one"
                        android:textColor="@color/white"
                        android:textSize="@dimen/bet_ball_size"

                        />
                    <com.uyt.ying.yuan.uuuu.iuymn.mkjnb.myView.MyCornerTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:text="10"
                        android:gravity="center"
                        android:id="@+id/happy10_num_two"
                        android:textColor="@color/white"
                        android:textSize="@dimen/bet_ball_size"
                        android:layout_marginLeft="8dp"

                        />
                    <com.uyt.ying.yuan.uuuu.iuymn.mkjnb.myView.MyCornerTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:text="10"
                        android:gravity="center"
                        android:id="@+id/happy10_num_three"
                        android:textColor="@color/white"
                        android:textSize="@dimen/bet_ball_size"
                        android:layout_marginLeft="8dp"
                        />
                    <com.uyt.ying.yuan.uuuu.iuymn.mkjnb.myView.MyCornerTextView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:text="10"
                        android:gravity="center"
                        android:id="@+id/happy10_num_four"
                        android:textColor="@color/white"
                        android:textSize="@dimen/bet_ball_size"
                        android:layout_marginLeft="8dp"
                        />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="3dp">
                <com.uyt.ying.yuan.uuuu.iuymn.mkjnb.myView.MyCornerTextView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:text="10"
                    android:gravity="center"
                    android:id="@+id/happy10_num_five"
                    android:textSize="@dimen/bet_ball_size"
                    android:textColor="@color/white"
                    />
                <com.uyt.ying.yuan.uuuu.iuymn.mkjnb.myView.MyCornerTextView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:text="10"
                    android:gravity="center"
                    android:id="@+id/happy10_num_six"
                    android:textColor="@color/white"
                    android:textSize="@dimen/bet_ball_size"
                    android:layout_marginLeft="8dp"
                    />
                <com.uyt.ying.yuan.uuuu.iuymn.mkjnb.myView.MyCornerTextView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:text="10"
                    android:gravity="center"
                    android:id="@+id/happy10_num_seven"
                    android:textColor="@color/white"
                    android:textSize="@dimen/bet_ball_size"
                    android:layout_marginLeft="8dp"
                    />
                <com.uyt.ying.yuan.uuuu.iuymn.mkjnb.myView.MyCornerTextView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:text="10"
                    android:gravity="center"
                    android:id="@+id/happy10_num_eight"
                    android:textColor="@color/white"
                    android:textSize="@dimen/bet_ball_size"
                    android:layout_marginLeft="8dp"
                    />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:text="107"
                android:textColor="#f66f2b"
                android:id="@+id/happy10_hezhi"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:text="107"
                android:textColor="@color/bigDanLong"
                android:id="@+id/happy10_daxiao"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:text="107"
                android:textColor="@color/bigDanLong"
                android:id="@+id/happy10_dashuang"
                />
        </LinearLayout>

    </LinearLayout>
    <include layout="@layout/split_horizontal"/>
</LinearLayout>