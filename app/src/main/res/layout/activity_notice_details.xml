<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".uuuu.iuymn.mkjnb.activity.mine_fragment_activitys.NoticeMessageDetailsActivity">
 <include layout="@layout/toolbar_layout"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp"
                android:textColor="@color/alpha_80_black"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="5dp"
                android:text="@string/隐私说明"
                android:layout_marginLeft="10dp"
                android:id="@+id/title"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:textColor="#999999"
                android:id="@+id/send_time"
                android:layout_marginLeft="12dp"
                android:text="dsdsdsd"
                android:layout_marginBottom="12dp"
                />
            <include layout="@layout/split_horizontal"/>
            <WebView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/notice_webview">
            </WebView>
        </LinearLayout>
    </ScrollView>

</LinearLayout>