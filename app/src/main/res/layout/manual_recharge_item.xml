<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/wrap_linear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="horizontal">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="7dp"
            >
            <TextView
                android:id="@+id/manual_name_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"

                android:text="- - -"
                android:textColor="@color/default_color"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/available_quota_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:maxWidth="112dp"
                android:singleLine="true"
                android:textColor="#666666"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="@+id/manual_name_tv"
                app:layout_constraintTop_toBottomOf="@+id/manual_name_tv" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#d8d8d8"
            />
            <LinearLayout
                android:id="@+id/qq_linear"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/white_normal_ripple"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_gravity="center"
                    >
                    <ImageView
                        android:id="@+id/qq_icon"
                        android:layout_width="17dp"
                        android:layout_height="17dp"
                        android:src="@drawable/qq"
                        android:layout_gravity="center"
                        />
                    <TextView
                        android:id="@+id/qq_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="12sp"
                        android:text="@string/立即联系"
                        android:textColor="#037EF3"
                        android:layout_gravity="center"
                        android:layout_marginTop="4dp"
                        />
                </LinearLayout>

            </LinearLayout>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#d8d8d8"
            />
        <LinearLayout
            android:id="@+id/weChat_linear"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/white_normal_ripple"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="center"
                >
                <ImageView
                    android:id="@+id/we_chat_icon"
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:src="@drawable/weix"
                    android:layout_gravity="center"
                    />
                <TextView
                    android:id="@+id/we_chat_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="12sp"
                    android:text="@string/立即联系"
                    android:layout_marginTop="4dp"
                    android:textColor="#02BB29"
                    android:layout_gravity="center"
                    />
            </LinearLayout>

        </LinearLayout>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#d8d8d8"
            />
        <LinearLayout
            android:id="@+id/aliPay_linear"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/white_normal_ripple"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="center"
                >
                <ImageView
                    android:id="@+id/ali_icon"
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:src="@drawable/zhifub"
                    android:layout_gravity="center"
                    />
                <TextView
                    android:id="@+id/ali_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="12sp"
                    android:text="@string/立即联系"
                    android:layout_marginTop="4dp"
                    android:textColor="#00AAEE"
                    android:layout_gravity="center"
                    />
            </LinearLayout>

        </LinearLayout>



    </LinearLayout>



    <TextView
        android:id="@+id/textView46"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#D8D8D8" />
</LinearLayout>